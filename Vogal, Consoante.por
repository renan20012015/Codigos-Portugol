programa {
  funcao inicio() {

    caracter letra

    escreva("Digite Uma Letra: ")
    leia(letra)

    se(letra == "a" ou letra == "e" ou letra == "i" ou letra == "o" ou letra == "u"){
    escreva("A Sua Letra É Uma Vogal ! ")
  }
    se(letra != "a" e letra != "e" e letra != "i" e letra != "o" e letra != "u"){
    escreva("A Sua Letra É Uma Consoante")

    }      
  }
}
