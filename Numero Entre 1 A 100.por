programa {
  funcao inicio() {

    inteiro numero1 = 1
    inteiro numero2 = 100 

    escreva("Digite Um Numero: ")
    leia(numero1)
    
    
    se(numero1 >=1)
    escreva("O número está entre 1 e 100")
    leia(numero2)

    se(numero2 <=100)
    escreva("O número não está entre 1 e 100")
  
  }
}
