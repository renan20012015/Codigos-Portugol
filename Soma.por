programa {
  funcao inicio() {

  inteiro A,B,C

  escreva("Digite o valor de A: ")
  leia(A)
  escreva("Digite o valor de B: ")
  leia(B)
  escreva("Digite o valor de C: ")
  leia(C)
    
  soma A + B

  escreva("A soma entre A e B é: ", A + B , "\n")
  se (A+B < C) 
  escreva("A soma é menor que C.")
  senao 
  escreva("A soma não é menor que C.")

  }
}
